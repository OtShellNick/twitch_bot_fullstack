{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": "./src",
    "paths": {
      "@api/*": ["./api/*"],
      "@uikit/*": ["./@setproduct-ui/core/*"],
      "@const/*": ["./const/*"],
      "@data/*": ["./data/*"],
      "@helpers/*": ["./helpers/*"],
      "@hooks/*": ["./hooks/*"],
      "@img/*": ["./img/*"],
      "@models/*": ["./models/*"],
      "@pages/*": ["./pages/*"],
      "@redux/*": ["./redux/*"],
      "@shared/*": ["./shared/*"],
      "@store/*": ["./store/*"],
      "@atoms/*": ["./ui/atoms/*"],
      "@blocks/*": ["./ui/blocks/*"],
      "@components/*": ["./ui/components/*"],
      "@templates/*": ["./ui/templates/*"],
    },
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ]
}
